{
  "name": "uni-bites",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "copy-bootstrap-files": "cp -r ./node_modules/bootstrap/dist ./public/bootstrap/",
    "populate-db": "node data/populate-db.js",
    "test": "protractor specs/conf.js",
    "test:once": "webdriver-manager update",
    "test:web-driver": "./node_modules/.bin/webdriver-manager start",
    "test:servers": "concurrently \"npm start\" \"npm run test:web-driver\"",
    "lint": "eslint --fix-dry-run",

    "docs": "gitbook serve docs",
    "docs:once": "gitbook install docs"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.2",
    "bootstrap": "^3.3.7",
    "cookie-parser": "^1.4.3",
    "debug": "^2.6.9",
    "express": "^4.15.5",
    "express-hbs": "^1.0.4",
    "hbs": "^4.0.1",
    "jsonwebtoken": "^8.1.1",
    "mongoose": "^4.13.11",
    "morgan": "^1.9.0",
    "nodemon": "^1.15.1",
    "serve-favicon": "^2.4.5"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "eslint": "^4.18.2",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "gitbook-cli": "^2.3.2",
    "jasmine": "^3.0.0",
    "mocha": "^5.0.1",
    "protractor": "^5.3.0"
  }
}
